{
    "require": {
        "born05/craft-assetusage": "4.1.3",
        "craftcms/ckeditor": "4.4.0",
        "craftcms/cms": "5.5.7",
        "craftcms/feed-me": "6.6.1",
        "dodecastudio/craft-blurhash": "^3.0.3",
        "ether/seo": "5.0.0",
        "nystudio107/craft-typogrify": "5.0.1",
        "nystudio107/craft-vite": "5.0.1",
        "nystudio107/craft-webperf": "5.0.0",
        "putyourlightson/craft-sherlock": "5.1.0",
        "putyourlightson/craft-sprig": "3.5.1",
        "verbb/expanded-singles": "3.0.1",
        "verbb/formie": "3.0.15",
        "verbb/hyper": "2.2.1",
        "vlucas/phpdotenv": "^5.4.1"
    },
    "require-dev": {
        "nystudio107/craft-autocomplete": "^1.11",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "treeware/plant": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        }
    ]
}
